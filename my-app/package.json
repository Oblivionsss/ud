{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "description": "Local version of Adaptive App",
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "docxtemplater": "^3.66.2",
    "express": "^5.1.0",
    "jszip": "^3.10.1",
    "pizzip": "^3.2.0",
    "prisma": "^6.15.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
